{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
        }
    },
    "rules": {
        "semi": 2,
        "no-use-before-define": [1, { "functions": false, "classes": true }],
        "camelcase": [0],
        "no-param-reassign": [2, { "props": false }]
    },
    "extends": "airbnb-base"
}
